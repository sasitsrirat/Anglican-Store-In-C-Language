#include <stdio.h>
#include <string.h>
void main ()
{
    char army[5][15],ch;
    char temp[15];
    int c,j,i;

    for(i=0; i<5; i++)
    {
        do
        {
            printf("Name%d : ",i+1); scanf("%s",army[i]);
            for(c=0,j=0; j<strlen(army[i]); j++)
            {
                army[i][j] = tolower(army[i][j]);
                //strlwr(army[i]);
                if(army[i][j]=='a' ||army[i][j]=='e'|| army[i][j]=='i'|| army[i][j]=='o' || army[i][j]=='u' || army[i][j]=='A'|| army[i][j]=='E'|| army[i][j]=='I'|| army[i][j]=='O'|| army[i][j]=='U')
                {
                    c++;
                    army[i][j] = toupper(army[i][j]);
                }
            }
        }
        while(strlen(army[i])!=6||c!=3);
    }

    for(i=0; i<4; i++)
    {
        for(j=i+1; j<5; j++)
        {
            if (strcmp(army[i],army[j])>0) // army[i] = army[j]
            {
                strcpy(temp,army[i]);
                strcpy(army[i],army[j]);
                strcpy(army[j],temp);
            }
        }
    }
    printf("KeronArmy : ");
    for(i=0; i<5; i++)
        printf(" %s",army[i]);
    printf("\n");
}
